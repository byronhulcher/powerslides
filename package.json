{
  "//1": "describes your app and its dependencies",
  "//2": "https://docs.npmjs.com/files/package.json",
  "//3": "updating this file will download and update your packages",
  "name": "twitter-image-downloader",
  "version": "0.1.0",
  "description": "Track the images posted by a Twitter account and optionally upload them to an S3 bucket",
  "scripts": {
    "track-media": "node ./lib/input/track-media.js",
    "upload-media": "node ./lib/input/upload-media.js",
    "clear-local-data": "rimraf .data && mkdirp .data/leveldb",
    "start": "npm run app",
    "app": "node server.js",
    "standard": "standard"
  },
  "dependencies": {
    "aws-sdk": "^2.275.1",
    "bignum": "^0.13.0",
    "express": "^4.16.3",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "twit": "^2.2.10",
    "level": "^4.0.0",
    "mkdirp": "^0.5.1",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": "10"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/twitter-image-downloader"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "standard": {
    "ignore": [
      "public/powerslides/draggydivs.js"
    ]
  }
}